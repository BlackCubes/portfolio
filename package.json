{
  "homepage": "https://blackcubes.github.io/portfolio",
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A portfolio website to showcase my skillsets and projects",
  "main": "index.js",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "watch:sass": "node-sass sass/main.scss css/compile/styles.css -w",
    "devserver": "live-server",
    "dev:sass": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/compile/styles.comp.css",
    "concat:css": "concat -o css/compile/styles.concat.css css/compile/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 6 versions\" css/compile/styles.concat.css -o css/compile/styles.prefix.css",
    "compress:css": "node-sass css/compile/styles.prefix.css css/bundle/styles.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlackCubes/portfolio.git"
  },
  "author": "Elias Gutierrez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlackCubes/portfolio/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "prettier": "^2.2.1",
    "snowpack": "^3.2.2"
  }
}
