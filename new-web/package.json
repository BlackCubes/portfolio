{
  "homepage": "https://www.eliastgutierrez.com/",
  "name": "portfolio",
  "version": "3.0.0",
  "description": "A portfolio website to showcase my skillsets, projects, and written articles. The original version was part of a final project for \"Mobile Friendly Class\" at Bitwise Industries, and the newer version was for my React Apprenticeship.",
  "author": "Elias Gutierrez",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd ../ && husky install ./new-web/.husky",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "better-react-mathjax": "^2.0.1",
    "framer-motion": "^6.3.6",
    "html-react-parser": "^1.4.14",
    "next": "12.1.6",
    "next-offline": "^5.0.5",
    "next-redux-wrapper": "^7.0.5",
    "next-sitemap": "^3.0.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-intersection-observer": "^9.2.0",
    "react-redux": "^8.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2",
    "typescript": "4.7.2"
  }
}
